<div class="search_form">
  <%= form_with url: search_path, class:"search_form_main", local: true, method: :get do |f| %>
    <%= f.text_field :key_word, class:"key_word_sub" %>
    <%= f.select :range, options_for_select([["旅行タイトル","Travel"], ["ユーザー名","User"]], class:"range_type") %>
    <%= f.select :search, options_for_select([["部分一致","partial_match"], ["前方一致","forward_match"], ["後方一致","backward_match"], ["完全一致","perfect_match"]], class:"serch_type") %>
    <%= f.submit "検索", class: "btn-primary" %>
  <% end %>
</div>